<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Kevin Jin</Authors>
    <Company>Magic Eastern Studio</Company>
    <Description>Prerequisite for MagicEastern.CachedFunc.Core and MagicEastern.CachedFunc.Net45.</Description>
    <PackageId>MagicEastern.CachedFunc.Base</PackageId>
    <PackageProjectUrl>https://github.com/jxdking/CachedFunc</PackageProjectUrl>
    <Version>2.0.0</Version>
    <Product>MagicEastern.CachedFunc.Base</Product>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>Copyright 2017-2019</Copyright>
    <AssemblyName>MagicEastern.CachedFunc.Base</AssemblyName>
    <RootNamespace>MagicEastern.CachedFunc.Base</RootNamespace>
    <PackageTags>Cache</PackageTags>
    <PackageReleaseNotes>Bug fixes. Fixed the crash when target function take none parameter.</PackageReleaseNotes>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

</Project>
